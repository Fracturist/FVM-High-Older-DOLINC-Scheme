FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;

    scheme          UENO3 grad(U) 1.2;
    schemeV         UENO3 grad(U) 1.2;

    div(phi,U)      Gauss $schemeV;

    div(phi,e)      Gauss $scheme;
    div(phi,K)      Gauss $scheme;
    div(phiv,p)     Gauss $scheme;

    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}


// ************************************************************************* //
